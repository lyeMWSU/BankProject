<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ComboBox?>
<GridPane fx:controller="UI.MainController"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">

    <Label text="Choose User Type:" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
    <ComboBox GridPane.rowIndex="1" GridPane.columnIndex="0" fx:id="UserType_ComboBox" GridPane.columnSpan="2" maxWidth="Infinity"/>
    <Button GridPane.rowIndex="2" GridPane.columnIndex="0" text="Exit" onMouseClicked="#Exit_OnMouseClick"/>
    <Button GridPane.rowIndex="2" GridPane.columnIndex="1" text="Load User" onMouseClicked="#LoadUser_OnMouseClick"/>

</GridPane>