<?xml version="1.0" encoding="UTF-8"?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>

<GridPane
        alignment="center"
        hgap="10"
        vgap="10"
        xmlns:fx="http://javafx.com/fxml/1"
        xmlns="http://javafx.com/javafx/8.0.121"
        fx:controller="UI.Withdraw.WithdrawController"
        prefHeight="400.0" prefWidth="600.0">

<Label
        GridPane.rowIndex="0"
        GridPane.columnIndex="0"
        text="Current Balance: "/>

<Text
        GridPane.rowIndex="0"
        GridPane.columnIndex="1"
        fx:id="CurrentBalance_Text"/>

<Label
        GridPane.rowIndex="1"
        GridPane.columnIndex="0"
        text="Withdraw Amount: "/>

<TextField
        GridPane.rowIndex="1"
        GridPane.columnIndex="1"
        fx:id="WithdrawAmount_TextField"/>

<Button
        GridPane.rowIndex="2"
        GridPane.columnIndex="0"
        text="Cancel"
        onMouseClicked="#Cancel_OnMouseClicked"/>

<Button
        GridPane.rowIndex="2"
        GridPane.columnIndex="1"
        text="Withdraw"
        onMouseClicked="#Withdraw_OnMouseClicked"/>

</GridPane>
